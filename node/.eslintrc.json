{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "node/no-unpublished-import": "off",
    "@typescript-eslint/no-non-null-assertion": "off"
  },
  "env": {
    "node": true
  }
}
